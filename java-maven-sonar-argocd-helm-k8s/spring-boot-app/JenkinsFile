pipeline {
  agent any
  stages {
    stage('Checkout') {
      steps {
        checkout([
          $class: 'GitSCM',
          branches: [[name: '*/main']],
          userRemoteConfigs: [[url: 'https://github.com/Yashsharma2004/jenkins.git']]
        ])
        sh 'echo Checkout Completed'
      }
    }
  }
}
